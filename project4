-- 게시글 작성 pboard
 create table pboard ( 
	bid			number(5), -- 게시글 id
 	writer		varchar2(40), -- 작성자
 	title		varchar2(40), -- 제목
 	content		varchar2(100), -- 내용
 	regdate		DATE, -- 등록일
 	hit			number(5), -- 좋아요
-- 	bcode 		number(10), --게시글 번호
 	PRIMARY KEY (bid) 
 );
 
 select * from pboard;
 
alter table board3 drop (bcode);
select * from board3;
delete from liketable;
